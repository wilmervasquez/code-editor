{
    "[css]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
      "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[java]": {
      "editor.defaultFormatter": "redhat.java"
    },
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[python]": {
      "editor.formatOnType": true
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[yaml]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "console-ninja.featureSet": "Community",
    "cSpell.language": "en,es-ES",
    "cSpell.userWords": [
      "calt",
      "Cascadia",
      "codepaths",
      "davinci",
      "dlig",
      "dracula",
      "esbenp",
      "gitbook",
      "importmap",
      "kubernetes",
      "monaco",
      "onum",
      "openai",
      "precompress",
      "redhat",
      "Stringf",
      "supabase",
      "Sysnative",
      "tsbuildinfo",
      "txtclave",
      "vskit",
      "wilmervasquezlino",
      "windir",
      "wkit"
    ],
    "css.validate": false, //tailwind postcss
    "dart.flutterRunLogFile": null,
    "dart.previewFlutterUiGuides": true,
    "dart.previewFlutterUiGuidesCustomTracking": true,
    "debug.console.fontFamily": "Cascadia Code",
    "debug.console.fontSize": 11,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontFamily": "Cascadia Code",
    "editor.fontLigatures": "'ss01', 'ss02','cv27', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07','ss09','cv31', 'calt', 'dlig','zero','onum'",
    "editor.fontSize": 13,
    "editor.fontVariations": true,
    "editor.fontWeight": "normal",
    "editor.formatOnSave": false,
    "editor.guides.bracketPairs": true,
    // "editor.guides.bracketPairsHorizontal": false,
    // "editor.guides.highlightActiveBracketPair": false,
    "editor.guides.highlightActiveIndentation": true,
    "editor.guides.indentation":true,
    "editor.hideCursorInOverviewRuler": true,
    "editor.indentSize": 2,
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.lineHeight": 1.4,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.overviewRulerBorder": false,
    "editor.parameterHints.enabled": true,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": false
    },
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.horizontal": "hidden",
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
      "enabled": true
    },
    "editor.stickyScroll.enabled": true,
    "editor.tabCompletion": "on",
    "editor.tabSize": 2,
    "editor.tokenColorCustomizations": {
      "[Dracula]": {
        "textMateRules": [
          {
            "scope": ["storage.type", "keyword"],
            "settings": {
              "fontStyle": "italic"
            }
          },
          {
            "scope": "entity.name.function",
            "settings": {
              "fontStyle": ""
            }
          }
        ]
      },
      "[idx-dark]": {
        "textMateRules": [
          {
            "scope": ["storage.type", "keyword"],
            "settings": {
              "fontStyle": "italic"
            }
          },
          {
            "scope": "entity.name.function",
            "settings": {
              "fontStyle": ""
            }
          }
        ]
      },
      "[One Dark Pro Darker]": {
        "textMateRules": [
          {
            "scope": ["storage.type", "keyword"],
            "settings": {
              "fontStyle": "italic"
            }
          },
          {
            "scope": "entity.name.function",
            "settings": {
              "fontStyle": ""
            }
          }
        ]
      }
    },
    "editor.wrappingIndent": "none",
    "emmet.extensionsPath": [
      "C:\\Users\\Wilmer\\AppData\\Roaming\\Code\\User\\snippets\\snippets.json"
    ],
    "errorLens.gutterIconsEnabled": true,
    "errorLens.gutterIconSet": "circle",
    "errorLens.gutterIconSize": "120%",
    "errorLens.messageEnabled": true,
    "errorLens.statusBarIconsEnabled": true,
    "errorLens.statusBarIconsTargetProblems": "activeEditor",
    "errorLens.statusBarIconsUseBackground": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
      "*.java": "${capture}.class",
      "*.js": "${capture}.js.map",
      "package.json": ".*"
    },
    "explorer.openEditors.sortOrder": "alphabetical",
    "explorer.openEditors.visible": 14,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "files.autoSave": "onFocusChange",
    "files.exclude": {
      // "**/.vscode": true
    },
    "git.openRepositoryInParentFolders": "never",
    "git.terminalGitEditor": true,
    "gitlens.ai.experimental.openai.model": "gpt-4-1106-preview",
    "gitlens.ai.experimental.provider": "openai",
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.validate.scripts": true,
    "html.validate.styles": true,
    "javascript.autoClosingTags": true,
    "javascript.inlayHints.parameterNames.enabled": "all",
    "javascript.suggest.autoImports": true,
    "js/ts.implicitProjectConfig.checkJs": true,
    "markdown.preview.fontFamily": "DM Sans",
    "material-icon-theme.folders.color": "#42a5f5",
    "material-icon-theme.hidesExplorerArrows": true,
    "oneDarkPro.bold": true,
    "oneDarkPro.editorTheme": "One Dark Pro Darker",
    "redhat.telemetry.enabled": true,
    "scm.inputFontFamily": "",
    "svelte.enable-ts-plugin": true,
    "svgPreview.autoOpen": false,
    "symbols.folders.associations": {},
    "symbols.hidesExplorerArrows": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorStyleInactive": "none",
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
    "terminal.integrated.env.windows": {},
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.fontWeightBold": "bold",
    "terminal.integrated.profiles.windows": {
      "Command Prompt": {
        "args": [],
        "icon": "terminal-cmd",
        "path": [
          "${env:windir}\\Sysnative\\cmd.exe",
          "${env:windir}\\System32\\cmd.exe"
        ]
      },
      "Git Bash": {
        "source": "Git Bash"
      },
      "PowerShell": {
        "icon": "terminal-powershell",
        "source": "PowerShell"
      },
      "Windows PowerShell": {
        "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
      }
    },
    "thunder-client.sidebar.treeIndentation": 5,
    "typescript.updateImportsOnFileMove.enabled": "never",
    "vs-kubernetes": {
      "vscode-kubernetes.helm-path-windows": "C:\\Users\\Wilmer\\.vs-kubernetes\\tools\\helm\\windows-amd64\\helm.exe",
      "vscode-kubernetes.kubectl-path-windows": "C:\\Users\\Wilmer\\.vs-kubernetes\\tools\\kubectl\\kubectl.exe"
    },
    "window.title": "${rootName}",
    "workbench.activityBar.location": "top",
    "workbench.colorCustomizations": {
      "focusBorder": "#ffffff15",
      "panel.border": "#ffffff15",
      "sideBar.background": "#21232c"
    },
    "workbench.iconTheme": "idx-icons",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.sideBar.location": "right",
    "workbench.tree.enableStickyScroll": true,
    "workbench.tree.renderIndentGuides": "always",
    "pixelbyte.love2d.srcDir": "",
    "pixelbyte.love2d.path": "C:\\Program Files\\LOVE\\love.exe",
    "pixelbyte.love2d.debug": true,
    "syntax.debugHover": true,
    "synthwave84.disableGlow": true,
    "workbench.colorTheme": "One Dark Pro Darker",
    "oneDarkPro.vivid": true,
    "editor.inlayHints.enabled": "off",
    "editor.glyphMargin": false,
    "[svelte]": {
      "editor.defaultFormatter": "svelte.svelte-vscode"
    }
  }
  